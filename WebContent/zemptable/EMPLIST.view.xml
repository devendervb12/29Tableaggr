<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="zemptable.EMPLIST" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		
	        <Table items="{/company/empInfo}">
	          <columns>	            
	             <Column>
	                <header>
	                    <Text text="Name"/>
	                </header>
	             </Column>
	             
	             <Column>	              
	                   <Text text="Role"/>	               
	             </Column>
	             
	             <Column>
	                <Text text="Department"/>
	             </Column>	             
	          </columns>
	          <items>
	             <ColumnListItem>
	                 <cells>
	                    <Text text="{name}"/>
	                    <Text text="{role}"/>
	                    <Text text="{dept}"/>
	                 </cells>
	             </ColumnListItem>
	          </items>
	          
	          
	        </Table>
		
	</Page>
</core:View>